{
  "name": "buttercli",
  "description": "A CLI to interact with the Butterfly User Manager REST API",
  "version": "0.0.1",
  "author": "DStack Group <dstackgroup@gmail.com>",
  "bin": {
    "buttercli": "./bin/run"
  },
  "repository": "https://github.com/dstack-group/Butterfly",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "tsc",
    "clean": "rimraf ./dist",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "prestandalone": "oclif-dev manifest && rimraf buttercli",
    "standalone": "pkg -t node10-linux-x64 .",
    "poststandalone": "rimraf oclif.manifest.json",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.7.0",
    "@oclif/config": "^1.16.0",
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/plugin-help": "^3.1.0",
    "@oclif/plugin-not-found": "^1.2.4",
    "cli-ux": "^5.4.9",
    "inquirer": "^7.3.2",
    "lowdb": "^1.0.0",
    "node-fetch": "^2.6.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@oclif/tslint": "^3.1.1",
    "@types/inquirer": "^6.5.0",
    "@types/lowdb": "^1.0.9",
    "@types/node": "^14.0.23",
    "@types/node-fetch": "^2.5.7",
    "globby": "^11.0.1",
    "lodash": "^4.17.19",
    "pkg": "^4.4.9",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "buttercli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ],
    "topics": {
      "config": {
        "description": "Manage buttercli settings"
      },
      "contact": {
        "description": "Perform contact related operations"
      },
      "project": {
        "description": "Perform project related operations"
      },
      "sub": {
        "description": "Perform subscription related operations"
      },
      "user": {
        "description": "Perform user related operations"
      }
    }
  },
  "types": "dist/index.d.ts"
}
